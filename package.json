{
  "name": "nextjs-drizzle-gen",
  "version": "0.2.1",
  "description": "Rails-like generators for Next.js + Drizzle ORM projects",
  "type": "module",
  "bin": {
    "drizzle-gen": "dist/index.js",
    "nextjs-drizzle-gen": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build && npm run test:run",
    "docs:dev": "cd docs && npm run start",
    "docs:build": "cd docs && npm run build"
  },
  "keywords": [
    "nextjs",
    "drizzle",
    "orm",
    "generator",
    "scaffold",
    "cli",
    "codegen"
  ],
  "author": "mantaskaveckas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantaskaveckas/nextjs-drizzle-gen.git"
  },
  "bugs": {
    "url": "https://github.com/mantaskaveckas/nextjs-drizzle-gen/issues"
  },
  "homepage": "https://github.com/mantaskaveckas/nextjs-drizzle-gen#readme",
  "dependencies": {
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsup": "^8",
    "typescript": "^5",
    "vitest": "^4"
  },
  "engines": {
    "node": ">=18"
  }
}
